ents/MovieListPage/styles.js",[],"/Users/joshsuson/rank-and-share/src/components/RankPage/index.js",[],"/Users/joshsuson/rank-and-share/src/components/RankPage/styles.js",[],"/Users/joshsuson/rank-and-share/src/components/MoviePoster/index.js",["115"],"import React, { useEffect, useState } from \"react\";\nimport { MoviePosterWrapper } from \"./styles\";\n\nexport const MoviePoster = ({ movie, handleClick }) => {\n  const baseImgUrl = \"https://image.tmdb.org/t/p/w185/\";\n  const [ranked, setRanked] = useState(false);\n  const [ranking, setRanking] = useState(null);\n\n  useEffect(() => {\n    setRanking(movie.userRanking);\n  }, [movie.userRanking, setRanking]);\n\n  const handleRanking = (movie) => {\n    handleClick(movie);\n    if (ranked) {\n      setRanked(false);\n    } else {\n      setRanked(true);\n    }\n  };\n\n  return (\n    <MoviePosterWrapper ranked={ranked} onClick={() => handleRanking(movie)}>\n      {ranked && <div>{ranking}</div>}\n      <img src={`${baseImgUrl}${movie.poster_path}`} />\n    </MoviePosterWrapper>\n  );\n};\n","/Users/joshsuson/rank-and-share/src/components/MoviePoster/styles.js",[],"/Users/joshsuson/rank-and-share/src/context/FirebaseContext.js",[],"/Users/joshsuson/rank-and-share/src/context/Firebase.js",[],"/Users/joshsuson/rank-and-share/src/components/SignUp/index.js",[],"/Users/joshsuson/rank-and-share/src/components/SignUp/styles.js",[],"/Users/joshsuson/rank-and-share/src/components/Button/index.js",[],"/Users/joshsuson/rank-and-share/src/components/Button/styles.js",[],{"ruleId":"116","replacedBy":"117"},{"ruleId":"118","replacedBy":"119"},{"ruleId":"120","severity":1,"message":"121","line":15,"column":31,"nodeType":"122","endLine":15,"endColumn":76},{"ruleId":"123","severity":1,"message":"124","line":1,"column":17,"nodeType":"125","messageId":"126","endLine":1,"endColumn":25},{"ruleId":"120","severity":1,"message":"121","line":12,"column":11,"nodeType":"122","endLine":12,"endColumn":60},{"ruleId":"120","severity":1,"mes